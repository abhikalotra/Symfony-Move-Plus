<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MovePlus\ServiceBundle\Entity\BlogGPolylines" table="blog_g_polylines">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="map" type="integer" column="map" nullable="false"/>
    <field name="name" type="string" column="name" length="50" nullable="false"/>
    <field name="data" type="text" column="data" nullable="false"/>
    <field name="lineOpacity" type="string" column="line_opacity" length="5" nullable="false"/>
    <field name="lineColor" type="string" column="line_color" length="7" nullable="false"/>
    <field name="lineWidth" type="string" column="line_width" length="5" nullable="false"/>
    <field name="hoverLineColor" type="string" column="hover_line_color" length="9" nullable="false"/>
    <field name="hoverLineOpacity" type="string" column="hover_line_opacity" length="9" nullable="false"/>
  </entity>
</doctrine-mapping>
