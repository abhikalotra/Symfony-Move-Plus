<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MovePlus\ServiceBundle\Entity\BlogGMarkers" table="blog_g_markers">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="map" type="integer" column="map" nullable="false"/>
    <field name="title" type="string" column="title" length="50" nullable="false"/>
    <field name="lat" type="string" column="lat" length="255" nullable="false"/>
    <field name="lng" type="string" column="lng" length="255" nullable="false"/>
    <field name="animation" type="string" column="animation" length="255" nullable="false"/>
    <field name="description" type="text" column="description" nullable="false"/>
    <field name="img" type="string" column="img" length="255" nullable="false"/>
    <field name="size" type="string" column="size" length="11" nullable="false"/>
  </entity>
</doctrine-mapping>
