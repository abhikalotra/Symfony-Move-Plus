<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="MovePlus\ServiceBundle\Entity\RegionArea" table="region_area">
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="area" type="string" column="area" length="255" nullable="true"/>
    <field name="postalCode" type="string" column="postal_code" length="255" nullable="true"/>
    <field name="status" type="integer" column="status" nullable="true"/>
    <field name="created" type="string" column="created" length="255" nullable="true"/>
    <field name="updated" type="datetime" column="updated" nullable="true"/>
  </entity>
</doctrine-mapping>
