
{% include '::header_service.html.twig' %}

    
    <div id="center_content">
    		<div class="left_content">
            
                    <h1>{{county}}  {{ title }} </h1>
                    
                 {% for county in countys  %}
						<p>
							{% if county.featuredImage %}
								<img src="{{ asset('uploads/serviceImage') }}/{{ county.featuredImage }}" alt="" title="" class="left_icon" />
							{% else  %}	
								<img src="{{ asset('theme/frontend/images') }}/81-300x198.jpg" alt="" title="" class="left_icon" />
							{% endif %}
							
						{{ county.shortDescription }}      Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua. 
						</p>
			
						<h1>{{county.county}}   {{ county.title }} </h1>
						<p>	
							{% if county.featuredImage %}											
								<img src="{{ asset('uploads/serviceImage') }}/{{ county.featuredImage }}" alt="" title="" class="right_icon" />
							{% else  %}	
								<img src="{{ asset('theme/frontend/images') }}/81-300x198.jpg" alt="" title="" class="right_icon" />
							{% endif %}	
									
							{{county.description }}  Lorem ipsum dolor sit amet, consectetur adipisicing elit, sed do eiusmod tempor incididunt ut labore et dolore magna aliqua.                    	
						</p>
                 {% endfor %}
                    
                    
                    <h1>Photos -----region</h1>
                    <div class="photos_block">                    
                    	{% for allService in allServiceDetails  %}
							<div class="photo_box">
								<a href="{{ path('move_plus_service_webService', {'slug':allService.slug }) }}" title="{{ allService.title }}" >
									<img src="{{ asset('uploads/serviceImage') }}/{{ allService.featuredImage }}" title="{{ allService.title }}" class="photo" border="0" /></a><br />
								
								 <a  href="{{ asset('app_dev.php') }}/{{ allService.regionArea }}/{{ allService.county }}/{{ allService.slug }}" title="{{ allService.regionArea }}"> {{ allService.regionArea }} </a> 
								 
								
								
								<!-- <a href="{{ path('move_plus_service_webService', {'slug':allService.title }) }}" class="view">{{ allService.title }}</a> -->
							</div>                    	                 
                    	 {% endfor %}	            
                    </div>
                			
                    
                      <div class="countryListShow">
						{% for countyService in regionListByCountyParentId  %}							
							<h3><ul class="list">
								<li>
								
								 <!-- <a  href="{{ asset('app_dev.php') }}/{{ countyService.regionArea }}/{{ countyService.county }}/{{ countyService.title }}" title="{{ countyService.regionArea }}"> {{ countyService.regionArea }} </a>  -->
								
								<a  href="{{ path('move_plus_service_regionArea', {'regionArea':countyService.regionArea, 'county':countyService.county, 'slug':countyService.slug, }) }}" title="{{ countyService.regionArea }}"> {{ countyService.regionArea }} </a> 
								
								
								
								</li>
							</ul></h3>
					{% endfor %}
                    </div>
            
            
					
            
            </div>
          
          
            <div class="right_content">
            
                     <h1>Services</h1>
                        
						{% include '::sidebar_header_allservice.html.twig' %}
						
					
            </div>
            
            
        
                
          
    
    

