  	  <!-- <script src="http://ajax.googleapis.com/ajax/libs/jquery/1.10.2/jquery.min.js"></script>   -->
	 <script type="text/javascript" src="{{ asset('theme/frontend/js/') }}jquery-ias.min.js"></script> 
     <script type="text/javascript">
        $(document).ready(function() {
        	// Infinite Ajax Scroll configuration
            jQuery.ias({
                container : '.wrap', // main container where data goes to append
                item: '.item', // single items
                pagination: '.navss', // page navigation
                next: '.navss a', // next page selector
                loader: '<div class="loaderWidthImage"><img src="{{ asset("theme/frontend/images/") }}loading.gif" style=" float: left;"> </div> ', // loading gif
                triggerPageThreshold: 3 // show load more if scroll more than this
            });
        });
    </script>   
	
<style>	 
.loaderWidthImage{
    float: left;
    margin-left: 37%;
	width: 100%;
}
</style>	 

<!-- blog section start here -->

<div class="row1">
  <div class="container">
    <div class="col-lg-12 text-center footer">
      <h1> City we cover </h1>
      <div class="container footerWrap">
        <div class="row">
        
       <!-- <ul class="list-unstyled">  -->
			<div class="wrap">
				<ul class="list-unstyled">
					  {% for city in cityPages %}
					
						 <li class="item" id="item-{{city.id}}" style="width:25%;margin:0px;margin-top:15px;">
								<a href="{{ path('move_plus_city_pages', { 'service_slug': city.service_slug,'county_slug': city.county_slug,'city_slug': city.city_slug }) }}"> {{city.city}}</a>
						  </li>
					  {% endfor %}
				 </ul>
		   </div>
       </div>
		
		{% if next  %}
			{% for city in cityPages  %}      
				<div class="navss">
					<a href="{{ path('move_plus_service_county', { 'county_slug': city.county_slug,'service_slug': city.service_slug }) }}?p={{ next }}">{{ next }}</a>
				</div>
			{% endfor %}
		{% endif %}

           
        </div>
      </div>
    </div>
  </div>
</div>


